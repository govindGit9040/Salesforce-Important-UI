import newstylePath from '@salesforce/resourceUrl/stylenew';
import {loadStyle} from 'lightning/platformResourceLoader';
// 
export default class OverrideDliLwc extends NavigationMixin(LightningElement) {
//    
    stylePath = newstylePath;
    constructor(){
        super();
        Promise.all([
            loadStyle(this, `${this.stylePath}/stylenew.css`)
        ])
    }





//////////
create stylenew.css file with style code and right click and create zip and upload and save in static resources as stylenew name
